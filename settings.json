{
  "package": {
    "name": "Wp Scaffold",
    "homepage": "https://github.com/Chrillaz/wordpress-theme-scaffold/",
    "author": "Christoffer Ã–hman",
    "authoruri": "https://github.com/Chrillaz/",
    "description": "Wordpress base theme to extend on",
    "version": "1.0.0",
    "license": "",
    "licenseuri": "",
    "textdomain": "scaffold",
    "domainpath": "",
    "template": ""
  },
  "webpack-assets": {
    "flags": {
      "jquery": false
    },
    "scripts": {
      "main": "./js/src/main.ts",
      "editor-scripts": "./js/src/editor.ts",
      "admin-scripts": "./js/src/admin.ts"
    },
    "styles": {
      "style": "./scss/main.scss",
      "editor-styles": "./scss/editor.scss",
      "admin-styles": "./scss/admin.scss"
    }
  },
  "settings": {
    "support": {
      "block-styles": true,
      "editor-styles": true,
      "align-wide": true,
      "responsive-embeds": true,
      "disable-custom-colors": true,
      "disable-custom-gradients": true,
      "disable-custom-font-sizes": false
    },
    "media-breakpoints": {
      "media.sm": "414px",
      "media.md": "768px",
      "media.lg": "992px",
      "media.xl": "1200px"
    },
    "color-palette": {
      "color.dark": "#232323",
      "color.primary-dark": "#31505E",
      "color.primary": "#446B7E",
      "color.primary-light": "#D2DCE0",
      "color.secondary-dark": "#ECE0D1",
      "color.secondary": "#F5EFE8",
      "color.secondary-light": "#EEEEEE",
      "color.light": "#FFFFFF"
    },
    "font-sizes": {
      "font.anchor": "12px",
      "font.paragraph": "16px",
      "font.h6": "20px",
      "font.h5": "24px",
      "font.h4": "28px",
      "font.h3": "32px",
      "font.h2": "36px",
      "font.h1": "40px"
    }
  },
  "block-types-exclude": [
    "core/columns",
    "core/column"
  ]
}